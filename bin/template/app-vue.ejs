<script setup>
<% if (!middleware.vueRouter) { %>
import {ref} from 'vue'
import HelloWorld from './components/HelloWorld.vue'
let msg = ref('Hello World ')
const changeMsgHandle = () => {
	msg.value += msg.value
}
<% } %>
</script>

<template>
	<div>
		<% if (middleware.vueRouter) { %>
			<router-view />
		<% } else { %>
			<h1>vue-cli</h1>
			<hello-world :msg="msg" @change="changeMsgHandle"/>
		<% } %>
	</div>
</template>

<style scoped></style>
	